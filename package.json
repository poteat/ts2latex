{
  "name": "ts2latex",
  "version": "1.0.8",
  "description": "Convert TypeScript to LaTeX algorithm format",
  "main": "src/ts2latex.mjs",
  "browser": "browser/ts2latex.min.js",
  "bin": {
    "ts2latex": "src/cmd.mjs"
  },
  "scripts": {
    "build": "node ./node_modules/rollup/bin/rollup -c",
    "test": "node --experimental-modules src/cmd.mjs test/1.ts"
  },
  "keywords": [
    "typescript",
    "latex",
    "convert",
    "format"
  ],
  "author": "mpoteat",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poteat/ts2latex.git"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.11",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4"
  }
}
